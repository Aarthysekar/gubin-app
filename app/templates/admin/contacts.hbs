<h1>Contacts</h1>

<table class="table table-bordered table-striped">
	<thead style="background-color:grey">
		<tr>
			<th>Email</th>
			<th>Message</th>
		</tr>
	</thead>
	<tbody>
	{{#each model as |contact|}}
		<tr>
			<th>{{contact.email}}</th>
			<th>{{contact.message}}</th>
		</tr>
	{{/each}}
	</tbody>
</table>


{{outlet}}
